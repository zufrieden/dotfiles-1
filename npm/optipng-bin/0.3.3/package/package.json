    "compress",
    "minify",
    "optimize",
    "img",
    "image"
  ],
  "author": {
    "name": "Sindre Sorhus",
    "email": "sindresorhus@gmail.com",
    "url": "http://sindresorhus.com"
  },
  "maintainers": [
    {
      "name": "Kevin Martensson",
      "email": "kevinmartensson@gmail.com",
      "url": "http://kevinmartensson.com"
    }
